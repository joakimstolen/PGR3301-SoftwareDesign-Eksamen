using System;
namespace SoftDesEksamen {
	public class SportsCar : ICar {


		private int _price;
		private string _type;
		private int _horsePower;
		private int _size;


		public SportsCar(int price, string type, int horsePower)
			{
			price = _price;
			type = _type;
			horsePower = _horsePower;
			}


		public double getPrice()
			{
			return _price;
			}
		string getType()
			{
			return _type;
			}
		int horsePower()
			{
			return _horsePower;
			}
		string getSeats()
			{
			return _seats;
			}


		}
	}
